<?xml version="1.0"?>
<extension key="nz.co.fuzion.icaltimezone" type="module">
  <file>icaltimezone</file>
  <name>Timezone for Event ICalendar</name>
  <description>Downloads the icalendar file as per timezone detected on user's device.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Jitendra Purohit</author>
    <email>jitendra@fuzion.co.nz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/fuzionnz/nz.co.fuzion.icaltimezone</url>
    <url desc="Documentation">https://github.com/fuzionnz/nz.co.fuzion.icaltimezone</url>
    <url desc="Support">https://github.com/fuzionnz/nz.co.fuzion.icaltimezone</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-10-25</releaseDate>
  <version>2.0.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Icaltimezone</namespace>
    <format>24.09.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.3</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
  </mixins>
</extension>
